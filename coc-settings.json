{
  "rpc.enabled": true,
  "rpc.workspaceElapsedTime": true,
  "rpc.idleTimeout": 600,
  "rpc.idleText": "Not Doing any Work",
  "rpc.showProblems": false,
  "rpc.detailsEditing": "{workspace_folder} {file_name}",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "django-html": "html"
  },
  "emmet.excludeLanguages": ["markdown", "json"],
  "eslint.nodePath": "/bin/node",
  "eslint.packageManager": "npm",
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "python",
    "html",
    "htmldjango",
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "Markdown",
    "python"
  ],
  "cSpell.userWords": ["htmldjango", "numpy", "pydocstring", "pylint"],
  "yaml.format.enable": true,
  "yaml.schemaStore.enable": true,
  "coc.preferences.extensionUpdateCheck": "daily"
}
